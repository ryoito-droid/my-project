{
  "name": "my-project",
  "version": "0.1.0",
  "description": "A Web Application project created with Miyabi",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
